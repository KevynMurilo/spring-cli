          "value": "6379"
        },
        {
          "key": "spring.data.redis.timeout",
          "value": "60000"
        },
        {
          "key": "spring.cache.type",
          "value": "redis"
        },
        {
          "key": "spring.cache.redis.time-to-live",
          "value": "600000"
        }
      ]
    },
    "infrastructure": {
      "dockerCompose": {
        "serviceName": "redis",
        "image": "redis:7-alpine",
        "ports": ["6379:6379"],
        "environment": {},
        "volumes": ["redis_data:/data"],
        "healthcheck": {
          "test": ["CMD", "redis-cli", "ping"],
          "interval": "10s",
          "timeout": "5s",
